<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>Terminal</icon>
	<name>Terminal</name>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description></description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>stock_folder</icon>
	<name>Thunar Root</name>
	<command>gksu thunar</command>
	<description>Thunar Root</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>system-search</icon>
	<name>Search</name>
	<command>catfish --fileman=thunar --path=%f</command>
	<description>find files and folders</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>/usr/share/icons/png/leafpad.png</icon>
	<name>Edit as Root</name>
	<command>gksu leafpad %f</command>
	<description>Edit as root user</description>
	<patterns>*</patterns>
	<other-files/>
	<text-files/>
</action>
<action>
	<icon>spacefm-find</icon>
	<name>Search with spaceFM</name>
	<command>spacefm --find-files %f</command>
	<description>Search with spaceFM</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
<action>
	<icon>roxterm</icon>
	<name>Open roxterminal</name>
	<command>roxterm %f</command>
	<description>Open roxterminal</description>
	<patterns>*</patterns>
	<directories/>
	<audio-files/>
	<image-files/>
	<other-files/>
	<text-files/>
	<video-files/>
</action>
</actions>
